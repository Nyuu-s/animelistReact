{
  "name": "animes",
  "version": "0.2.1",
  "private": true,
  "description": "Animes list imported from excel files ",
  "author": {
    "name": "rox"
  },
  "dependencies": {
    "@electron/remote": "^0.0.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "@mui/x-data-grid": "^5.15.0",
    "@syncfusion/ej2": "^20.2.38",
    "@syncfusion/ej2-base": "^20.2.38",
    "@syncfusion/ej2-react-calendars": "^20.2.38",
    "@syncfusion/ej2-react-grids": "^20.2.38",
    "@syncfusion/ej2-react-inputs": "^20.2.38",
    "@syncfusion/ej2-react-kanban": "^20.2.38",
    "@syncfusion/ej2-react-popups": "^20.2.38",
    "@syncfusion/ej2-react-schedule": "^20.2.38",
    "@syncfusion/ej2-react-splitbuttons": "^20.2.45",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "electron-log": "^4.4.8",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.2.1",
    "exceljs": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-table": "^7.8.0",
    "react-toastify": "^9.0.8",
    "web-vitals": "^2.1.4"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e:start": "electron .",
    "e:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "e:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run e:start\"",
    "e:publish": "npm run build && electron-builder -c.extraMetadata.main=build/main.js -p always"
  },
  "build": {
    "appId": "WeebAnimesList",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        {
          "provider": "github",
          "owner": "Nyuu-s",
          "repo": "animelistReact"
        }
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "AnimesV8_Uninstall",
      "license": "license.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.3.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "wait-on": "^6.0.1"
  }
}
